{
    "name": "textgen-ui",
	"address": "http://127.0.0.1:5000/api/v1/",
	"model": "model",
    "generation": "generate",
    "headers": {
        "Accept": "application/json",
        "Content-Type": "application/json"
    },
    "parameters": {
        "add_bos_token": "True",
        "ban_eos_token": "False",
        "do_sample": "True",
        "early_stopping": "True",
		"epsilon_cutoff": 0,
		"eta_cutoff": 0,
        "length_penalty": 1,
        "max_new_tokens": 400,
        "min_length": 5,
		"mirostat_mode": 2,
		"mirostat_tau": 5.0,
		"mirostat_eta": 0.2,
        "no_repeat_ngram_size": 0,
        "num_beams": 1,
        "penalty_alpha": 0,
		"preset": "None",
        "repetition_penalty": 1.15,
        "seed": -1,
        "skip_special_tokens": "True",
        "temperature": 0.7,
		"tfs": 1,
        "top_a": 0.2,
        "top_k": 40,
        "top_p": 0.75,
        "truncation_length": 2048,
        "typical_p": 1
    }
}